journal = {Annual Review of Condensed Matter Physics},
keywords = {fluctuation theorems,irreversibility,nonequilibrium,second law of thermodynamics,second law thermodynamics,work relations},
number = {1},
pages = {329--351},
title = {{Equalities and Inequalities: Irreversibility and the Second Law of Thermodynamics at the Nanoscale}},
