title = {{Statistical properties of entropy production derived from fluctuation theorems}},
url = {http://iopscience.iop.org/article/10.1088/1742-5468/2010/12/P12022$\backslash$nhttp://stacks.iop.org/1742-5468/2010/i=12/a=P12022?key=crossref.15c5d550bbf3e3fa3e017b9e703074e5},
