pages = {29--62},
title = {{On the Origin and the Use of Fluctuation Relations for the Entropy}},
url = {http://adsabs.harvard.edu/cgi-bin/nph-data{\_}query?bibcode=2004poin.book..145M{\&}link{\_}type=EJOURNAL$\backslash$npapers3://publication/doi/10.1007/978-3-0348-7932-3{\_}8},
volume = {2},
year = {2003}
}
