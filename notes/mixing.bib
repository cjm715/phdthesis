@article{Okabe2008,
abstract = {Mixing refers to the homogenization of$\backslash$nconcentrations of passive scalars in fluids. On$\backslash$nsmall scales it is dominated by diffusion and on$\backslash$nlarge scales it is assisted by stirring. In the$\backslash$npresence of scalar sources and sinks the$\backslash$nconcentration field remains inhomogeneous, but the$\backslash$ncombined effect of stirring and dissipation may lead$\backslash$nto a statistically stationary state. One measure of$\backslash$nthe quality of mixing is then the standard deviation$\backslash$nof the scalar concentration from the mean, and the$\backslash$neffectiveness of a stirring velocity field can be$\backslash$ngauged by comparing the concentration fluctuations$\backslash$nin the presence of stirring to those in its$\backslash$nabsence. It was recently noted that the maximum$\backslash$npossible effectiveness of any stirring depends on$\backslash$nthe detailed structure of the sources and sinks. We$\backslash$npresent results from particle-based simulations that$\backslash$nconfirm this strong source-sink dependence of the$\backslash$nmixing enhancement by stirring.},
archivePrefix = {arXiv},
arxivId = {0804.2805},
